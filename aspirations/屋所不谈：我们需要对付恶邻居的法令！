	(function($){
		$(document).on('scroll', function() {
	        if ($(window).scrollTop() >= 300) {
	           $('.header').addClass('headertotop');
	           // console.log('headertotop appear');
	        } else {
	           $('.header.headertotop').removeClass('headertotop');
